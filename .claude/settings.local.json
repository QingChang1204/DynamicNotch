{
  "permissions": {
    "allow": [
      "Bash(codesign:*)",
      "Bash(/usr/bin/codesign:*)",
      "Bash(hdiutil create:*)",
      "Bash(nc:*)",
      "Read(//Users/liuqingchang/Library/Developer/Xcode/DerivedData/**)",
      "Bash(cat:*)",
      "mcp__notchnoti__notch_show_result",
      "mcp__notchnoti__notch_show_actionable_result",
      "mcp__notchnoti__notch_ask_confirmation",
      "mcp__notchnoti__notch_show_summary"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Applications/NotchNoti.app/Contents/MacOS/notch-hook",
            "timeout": 2
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Applications/NotchNoti.app/Contents/MacOS/notch-hook",
            "timeout": 2
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Applications/NotchNoti.app/Contents/MacOS/notch-hook",
            "timeout": 2
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Applications/NotchNoti.app/Contents/MacOS/notch-hook",
            "timeout": 2
          }
        ]
      }
    ],
    "SessionStart": [
